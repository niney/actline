{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel. Single Compoent",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "niney",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niney/actline.git"
  },
  "homepage": "https://github.com/niney/actline",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "start-server-prod": "node express.js",
    "start-dev-part-detail-app": "webpack serve --config=configs/webpack/dev.js --env filename=parts-detail-app.tsx",
    "start-build-part-detail-app": "cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js --env filename=parts-detail-app.tsx",
    "start-dev-part-detail-open-market-app": "webpack serve --config=configs/webpack/dev.js --env filename=parts-detail-open-market-app.tsx",
    "start-build-part-detail-open-market-app": "cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js --env filename=parts-detail-open-market-app.tsx",
    "start-dev-part-form-app": "webpack serve --config=configs/webpack/dev.js --env filename=parts-form-app.tsx",
    "start-build-part-form-app": "cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js --env filename=parts-form-app.tsx",
    "start-dev-member-form-app": "webpack serve --config=configs/webpack/dev.js --env filename=member-form-app.tsx",
    "start-build-member-form-app": "cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js --env filename=member-form-app.tsx",
    "start-dev-bom-order-detail-app": "webpack serve --config=configs/webpack/dev.js --env filename=bom-order-detail-app.tsx",
    "start-build-bom-order-detail-app": "cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js --env filename=bom-order-detail-app.tsx",
    "start-prod-serve": "node express.js",
    "apollo:codegen": "rimraf src/__generated__ && apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.5",
    "@fullhuman/postcnss-purgecss": "fullhuman/postcss-purgecss",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.14",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "air-datepicker": "^3.3.5",
    "apollo": "^2.33.6",
    "graphql": "^15.5.1",
    "react-hook-form": "^7.14.0",
    "whatwg-fetch": "^3.6.2"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  }
}
